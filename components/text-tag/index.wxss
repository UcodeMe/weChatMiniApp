.text-tag {
  display: flex;
  flex-direction: row;
  flex-wrap: warp;
  -webkit-flex-wrap: wrap;
}

.text-tag text {
  display: block;
  padding: 1rpx 12rpx;
  height: 33rpx;
  line-height: 30rpx;
  margin-right: 22rpx;
  margin-top: 4rpx;
  border-radius: 20rpx;
  background: #fff9f1;
  font-size: 22rpx;
  font-weight: 400;
  color: #f57921;
}
