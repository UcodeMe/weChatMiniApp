.page-line {
  width: 100%;
  height: 20rpx;
  background: #eee;
}

.clearfix:after {
  display: block;
  clear: both;
  content: "";
  visibility: hidden;
  height: 0;
}

.company {
  padding: 20rpx 44rpx 20rpx 42rpx;
  display: flex;
  align-items: flex-start;
}

.companyLogo {
  width: 82rpx;
  height: 82rpx;
  border: 1rpx #ccc solid;
}

.company .doc-icon {
  width: 200rpx;
  /* height: 200rpx; */
  border: 1rpx solid#E5E5E5;
  padding: 10rpx;
  height: 90rpx;
}

.company .iconfont {
  font-size: 100rpx;
}

.company .middle {
  /* min-width: 60%; */
  max-width: 80%;
  float: left;
  margin-left: 20rpx;
  line-height: 40rpx;
  /* overflow:hidden; */
}

.companyName {
  font-size: 32rpx;
  font-weight: bold;
  max-width: 370rpx;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-right: 10rpx;
  line-height: 1;
}

.middle .type {
  font-size: 24rpx;
  width: 164rpx;
  height: 34rpx;
  line-height: 34rpx;
  text-align: center;
  border-radius: 15rpx;
  float: left;
  background: #ff9244;
  color: #fff;
}

.company .bottom {
  display: inline-flex;
  margin-top: 19rpx;
}

.bottom .natureName, .bottom .industryNames {
  color: #999;
  font-size: 24rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  max-width: 300rpx;
}

.natureName-line {
  float: left;
  width: 1rpx;
  height: 22rpx;
  margin-top: 6rpx;
  margin-right: 20rpx;
  margin-left: 20rpx;
  background: #999;
}

.tagNames {
  padding-left: 21rpx;
  padding-right: 21rpx;
  margin-left: 21rpx;
}

.tagTitle {
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  background: #fff !important;
  padding-left: 0 !important;
  padding-right: 0 !important;
}

.tagNames .tags1 {
  font-size: 22rpx;
  color: #f57921;
  /* margin-top: 6rpx; */
}

.tags1>view {
  height: 33rpx;
  line-height: 33rpx;
  padding: 6rpx 24rpx;
  border-radius: 33rpx;
  background-color: #fff9f1;
  text-align: center;
  float: left;
  margin-top: 30rpx;
}

.company-profile {
  margin-left: 41rpx;
  margin-right: 41rpx;
  padding-top: 40rpx;
  margin-bottom: 30rpx;
  position: relative;
  overflow-y: hidden;
  overflow-x: hidden;
  clear: both;
}

.company-profile .profile-title {
  color: #666;
  font-weight: bold;
  font-size: 30rpx;
}

.company-profile.detail {
  margin-top: 30rpx;
}

.follow {
  display: inline-block;
  height: 58rpx;
  width: 152rpx;
  color: #fff;
  text-align: center;
  background: #47bcb8;
  font-size: 32rpx;
  border-radius: 32rpx;
  line-height: 58rpx;
}

.unFollow {
  display: inline-block;
  height: 58rpx;
  width: 152rpx;
  color: #47bcb8;
  text-align: center;
  background: #fff;
  font-size: 32rpx;
  border-radius: 26rpx;
  line-height: 58rpx;
  border: 1rpx solid #47bcb8;
}

.profile-text {
  margin-top: 18rpx;
  line-height: 1.8;
  color: #666;
  font-size: 30rpx;
  overflow-y: hidden;
  text-overflow: ellipsis;
  /* white-space: nowrap; */
  overflow-x: hidden;
}

.show-more {
  width: 676rpx;
  padding-left: 146rpx;
  height: auto;
  box-sizing: border-box;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}

.more-line {
  width: 94rpx;
  height: 2rpx;
  float: left;
  background: #47bcb8;
  margin-top: 20rpx;
}

.more-text {
  width: 120rpx;
  /* height: 30rpx; */
  color: #47bcb8;
  text-align: center;
  font-size: 30rpx;
  margin-left: 32rpx;
  margin-right: 32rpx;
  float: left;
}

.timetable {
  width: 100%;
  padding-left: 24rpx;
  box-sizing: border-box;
}

.timetable-title {
  color: #666;
  font-size: 30rpx;
  margin-left: 8rpx;
  margin-top: 48rpx;
  font-weight: bolder;
}

.timetable-list {
  width: 100%;
  height: 142rpx;
  padding-top: 24rpx;
  box-sizing: border-box;
}

.list-M {
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  border: 2rpx solid #ebebeb;
  border-radius: 10rpx;
  float: left;
  margin-top: 12rpx;
}

.list-M>view {
  color: 80rpx;
  font-size: 38rpx;
  float: left;
  min-width: 50rpx;
  text-align: center;
  margin-left: 3rpx;
  margin-top: 16rpx;
}

.list-M>text {
  color: #999;
  font-size: 24rpx;
  float: left;
  margin-top: 28rpx;
}

.timetable-right {
  float: left;
  padding-left: 24rpx;
  box-sizing: border-box;
  width: 82%;
}

.list-date, .list-week {
  display: flex;
  justify-content: space-between;
  width: 100%;
}

.list-d, .list-w {
  float: left;
}

.list-w {
  color: #999;
  margin-bottom: 20rpx;
  padding: 0rpx 12rpx;
}

.list-d {
  width: 26rpx;
  height: 26rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 26rpx;
  color: #666;
  padding: 12rpx;
}

.active {
  background: #ff9244;
  color: white;
}

.meet-list {
  max-width: 97%;
  font-size: 30rpx;
  color: #999;
  padding-left: 8rpx;
  box-sizing: border-box;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  border-bottom: 1rpx solid #ebebeb;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.meet-time {
  margin-bottom: 10rpx;
}

.meet-place {
  width: 100%;
}

.place-L, .place-R, .place-title, .place-line {
  float: left;
}

.place-R {
  max-width: 66%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  box-sizing: border-box;
}

.place-line {
  height: 28rpx;
  width: 2rpx;
  background: #666;
  box-sizing: border-box;
  margin-top: 9rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.workflow {
  padding-left: 8rpx;
  margin-top: 20rpx;
  width: 94%;
  box-sizing: border-box;
  color: #666;
  font-size: 30rpx;
}

.workflow-title {
  font-weight: bolder;
  color: #666;
  margin-bottom: 50rpx;
}

.list-line {
  width: 39rpx;
  height: 2rpx;
  box-sizing: border-box;
  border: 2rpx solid #47bcb8;
  margin-top: -30rpx;
}

.inte-timel {
  padding-left: 8rpx;
  margin-top: 20rpx;
  width: 94%;
  box-sizing: border-box;
  color: #666;
  font-size: 30rpx;
  /* padding-bottom: 20rpx;
  border-bottom: 1rpx solid #999; */
}

.inte-title {
  font-weight: bolder;
  color: #666;
}

.inte-text {
  width: 100%;
  text-align: center;
  color: #ff9244;
  font-size: 30rpx;
  display: flex;
  line-height: 3rpx;
  justify-content: center;
  margin-top: 46rpx;
  font-weight: bold;
}

.jod-list {
  /* height: 216rpx; */
  width: 100%;
  box-sizing: border-box;
  padding: 46rpx 40rpx 20rpx 40rpx;
  margin-bottom: 120rpx;
}

.job-line {
  width: 706rpx;
  height: 2rpx;
  box-sizing: border-box;
  background: #ebebeb;
  margin-bottom: 46rpx;
  /* margin-top: 40rpx; */
  margin-left: -18rpx;
}

.job-list-left {
  margin-bottom: 28rpx;
}

.job-list-right {
  width: 155rpx;
  padding-top: 10rpx;
  margin-left: 10rpx;
  box-sizing: border-box;
  height: 100%;
}

.job-nam {
  float: left;
  width: 355rpx;
  color: #333;
  font-size: 32rpx;
  font-weight: bolder;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.job-pay {
  float: left;
  width: 28%;
  color: #47bcb8;
  font-size: 32rpx;
  margin-right: 2rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-weight: bold;
}

.job-address, .have-num, .job-conditions {
  float: left;
  color: #999;
  font-size: 24rpx;
}

.job-address, .have-num {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 28rpx;
  margin-bottom: 20rpx;
}

.job-address {
  width: 70%;
}

.job-conditions {
  max-width: 100%;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.get-button {
  width: 156rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  background: #47bcb8;
  border-radius: 32px;
}

.industryPositionName {
  font-size: 24rpx;
  width: 164rpx;
  height: 34rpx;
  line-height: 34rpx;
  text-align: center;
  border-radius: 15rpx;
  float: left;
  background: #ff9244;
  color: #fff;
}

.company-right {
  margin-left: 15rpx;
}

.company-right .title {
  display: flex;
}

.profile-top {
  height: 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.long-line {
  width: 40rpx;
  height: 3rpx;
  background: #ff9244;
  display: block;
  margin-left: 15px;
  margin-right: 15rpx;
}

.no-publicize {
  font-size: 30rpx;
  font-weight: 300;
  color: rgba(102, 102, 102, 1);
  line-height: 60px;
  width: 100%;
  text-align: center;
}

.job-block {
  display: flex;
}

.wxParse-p >image {
  width: 100% !important;
}

.follow-qrcode {
  width: 100%;
  height: 100%;
  position: fixed;
  display: flex;
  justify-content: center;
  align-items: center;
  background: rgba(0, 0, 0, 0.2);
  z-index: 999;
}

.follow-qrcode .inner {
  width: 600rpx;
  height: 700rpx;
  background: #fff;
  border-radius: 20rpx;
  z-index: 9999999;
  display: absolute;
  padding-top: 54rpx;
  padding-left: 48rpx;
  padding-right: 48rpx;
  margin-left: 40rpx;
  margin-right: 40rpx;
}

.follow-qrcode .title {
  font-size: 30rpx;
  font-weight: bold;
  line-height: 43rpx;
  widows: 100%;
  text-align: center;
  color: rgba(51, 51, 51, 1);
}

.follow-qrcode .help-name {
  font-size: 24rpx;
  font-weight: 400;
  line-height: 43rpx;
  color: rgba(170, 170, 170, 1);
  width: 100%;
  text-align: center;
  margin-top: 18rpx;
}

.follow-qrcode .btn-download {
  width: 408rpx;
  height: 62rpx;
  background: rgba(71, 188, 184, 1);
  border-radius: 50px;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 60rpx;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  margin-top: 30rpx;
}

.follow-qrcode .follow-name {
  font-size: 24rpx;
  font-weight: 400;
  line-height: 43rpx;
  color: rgba(170, 170, 170, 1);
  widows: 100%;
  text-align: center;
  margin-top: 18rpx;
}

.follow-qrcode .using-tips {
  font-size: 28rpx;
  font-weight: 400;
  line-height: 43rpx;
  text-align: center;
  widows: 100%;
  color: rgba(255, 146, 68, 1);
  margin-top: 53rpx;
}

.follow-qrcode .block-btn {
  margin-top: 18rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}

.follow-qrcode image {
  width: 247rpx;
  height: 247rpx;
  background: rgba(0, 0, 0, 0);
  border: 4px solid rgba(238, 238, 238, 1);
}

.icon-delete {
  font-size: 32rpx;
  z-index: 99999;
}
